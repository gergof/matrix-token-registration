{
  "name": "matrix-token-registration",
  "version": "0.1.0",
  "description": "Register to a matrix homeserver using a registration token and join rooms automatically.",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --watchAll",
    "test:coverage": "jest --collectCoverage --passWithNoTests --verbose",
    "lint": "eslint 'packages/backend/src/**/*.js' 'packages/frontend/src/**/*.js'",
    "format": "prettier --write 'packages/*/src/**/*.js'",
    "start": "lerna run start --stream",
    "build": "cross-env NODE_ENV=production lerna run build --stream",
    "bootstrap": "lerna bootstrap",
    "release": "lerna version --conventional-commits"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gergof/matrix-token-registration.git"
  },
  "keywords": [
    "matrix",
    "token",
    "registration",
    "join"
  ],
  "author": "Fándly Gergő-Zoltán <gergo@systemtest.tk>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/gergof/matrix-token-registration/issues"
  },
  "homepage": "https://github.com/gergof/matrix-token-registration#readme",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.1.0",
    "lerna": "^3.20.2",
    "prettier": "^1.19.1"
  },
  "dependencies": {}
}
