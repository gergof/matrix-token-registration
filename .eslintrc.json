{
	"env": {
		"es6": true,
		"node": true,
		"jest": true,
		"browser": true
	},
	"extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"settings": {
		"react": {
			"version": "16"
		}
	},
	"rules": {
		"no-console": "error",
		"prettier/prettier": "error"
	}
}
